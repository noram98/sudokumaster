{
  "name": "sudoku",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "rimraf src/.next && cross-env NODE_ENV=production next build src",
    "dev": "next src",
    "export": "rimraf src/out && yarn build && next export src",
    "lint": "tsc --build ./src/tsconfig.json && tslint --project ./src/tsconfig.json -t stylish 'src/**/*.{ts,tsx}' --fix && yarn prettier",
    "postinstall": "rm -rf node_modules/@types/react-native && patch-package",
    "prettier": "prettier src/**/*.{ts,tsx} --write",
    "prod": "cross-env NODE_ENV=production yarn build && yarn start",
    "start": "next start src"
  },
  "dependencies": {
    "@emotion/core": "^10.0.17",
    "@emotion/styled": "^10.0.17",
    "@styled-system/should-forward-prop": "^5.1.2",
    "deepmerge": "^4.0.0",
    "emotion-theming": "^10.0.19",
    "next": "^9.0.6",
    "nprogress": "^0.2.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "styled-icons": "^8.4.2",
    "styled-system": "^5.1.2"
  },
  "devDependencies": {
    "@types/node": "^12.7.5",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^16.9.2",
    "@types/styled-system": "^5.1.1",
    "babel-plugin-emotion": "^10.0.19",
    "babel-plugin-lodash": "^3.3.4",
    "convert-tsconfig-paths-to-webpack-aliases": "^0.9.2",
    "cross-env": "^6.0.0",
    "lodash-webpack-plugin": "^0.11.5",
    "patch-package": "^6.2.0",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "serve": "^11.1.0",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-language-service": "^0.9.9",
    "tslint-react": "^4.1.0",
    "typescript": "^3.6.3",
    "typescript-styled-plugin": "^0.14.0"
  }
}
